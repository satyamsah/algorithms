
import java.util.LinkedList;
import java.util.List;

public class BST {
	
	static Node root;
	class Node{
		Node left;
		Node right;
		int val;
		Node(int val){
			this.val=val;
		}
	}
	static void createBST(List<Integer> list){
		createBSTUtil(list,0,Node root);
	}
	

	public static void main(String[] args) {
		  List<Integer> list = new LinkedList<>();
		  list.add(1);
		  list.add(2);
		  list.add(3);
		  createBST(list);
	        

	}

}
